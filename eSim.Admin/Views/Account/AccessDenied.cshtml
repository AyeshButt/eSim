@model ErrorViewModel
@{
    ViewData["Title"] = "Error";
}
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<link href="~/btsui/assets/css/customcss.css" rel="stylesheet" />

<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">




<body style="margin: 0; height: 100vh; display: flex; justify-content: center; align-items: center; background-color: #212837; color: white;">

    <div class="custom-error-page">
        <div class="custom-error-page-content" style="color: white;">
            <div class="custom-error-img main-div">
                <div class="custom-error-img-code inner-div">401</div>
                <img src="/UserImages/404.svg" alt="">

            </div>


            <div class="d-flex flex-column justify-content-center align-items-center text-white text-center">
                <h6 class="display-3 text-warning" style="margin-top: 0; margin-bottom: 0;">Oops, Unauthorized!</h6>

                <a asp-action="Index" asp-controller="Home" class="btn btn-primary mt-3 mb-1">Go to Home</a>
            </div>


        </div>
    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
</body>

</html>


